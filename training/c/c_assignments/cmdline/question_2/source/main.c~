#include"header.h"

int main(int argc, char *argv[])
{
		int i;
		int count = 0;
		printf("\n first argument is %s ",argv[0]);
		printf("%d",argc);
		for(i = 1;i < argc; i++)  {
		if((*(&argv + i + str_len(argv[i]) - 1) == 'c' ) && 
				(*(&argv + i + str_len(argv[i]) - 2) == '.' ))

			printf("\n %d . %s ",count + 1,argv[i]);
			count++;
		}
		return 0;
}
